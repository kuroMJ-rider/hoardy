{
  "name": "hoardy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 127.0.0.1",
    "dev:all": "next dev -H 0.0.0.0",
    "dev:webpack": "next dev -H 127.0.0.1 --webpack",
